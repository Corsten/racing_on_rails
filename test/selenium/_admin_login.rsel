open "/user_session/new"
assert_text_not_present "error"
assert_location "*/user_session/new"
assert_text_present "Welcome"
assert_element_present "user_session_email"
assert_element_present "user_session_password"
type "user_session_email", "admin@example.com"
type "user_session_password", "secret"
click_and_wait "name=login"
assert_text_not_present "error"
assert_location "*/admin/events"
