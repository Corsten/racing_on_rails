<div class="modal fade" tabindex="-1" role="dialog" id="<%= target %>_select_modal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <%= form_tag({}, { method: "get", role: "form", id: "#{target}_select_modal_form" }) do %>
          <div class="form-group">
            <div class="input-group">
              <%= search_field_tag("name", @event.promoter.name, class: "wants_focus form-control", placeholder: "Enter name", autocomplete: "off") %>
              <span class="input-group-btn">
                <button type="submit" class="btn btn-default"><span class="glyphicon glyphicon-search"></span></button>
              </span>
            </div>
          </div>
        <% end -%>
      </div>
      <div class="modal-body">
        <table class="table select-modal table-hover" id="people">
          <tbody>
            <tr>
              <td class="empty">No results</td>
            </tr>
          </tbody>
        </table>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default">New Person</button>
        <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
      </div>
    </div>
  </div>
</div>
