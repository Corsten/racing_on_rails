  <% table :caption => "Single Day Events", :class => "box-3 last" do %>
    <tr>
      <%- events.each do |single_day_event| -%>
      <td class='nowrap' style="width: 120px;"><%= link_to single_day_event.date.strftime('%A, %B %d'), edit_admin_event_path(single_day_event) %></td>
      <td class='link'><%= link_to single_day_event.name, edit_admin_event_path(single_day_event) %></td>
    </tr>
    <%- end -%>
  <% end -%>
