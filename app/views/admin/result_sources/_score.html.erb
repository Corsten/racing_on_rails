<tr class="result_sources_<%= @result.id %>">
  <td></td>
  <td class="place"><small><%= result_source.source_result.place %></small></td>
  <td class="result">
    <div class="event"><small><%= result_source.source_result.event.name %></small></div>
  </td>
  <td><small><%= result_source.source_result.race.name %></small></td>
  <td><small><%= result_source.source_result.event.date.strftime("%D") %></small></td>
  <td><small><%= number_with_precision(result_source.points, precision: 0) %></small></td>
  <td></td>
</tr>
