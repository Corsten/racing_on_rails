<% if @destroyed %>
  jQuery('#race_<%= @race.id %>_row').hide('fade');
  jQuery('#warn').hide();
  jQuery('#notice').show();
  jQuery('#notice_span').html('Deleted <%= @race.name %>');
<% else %>
  jQuery('#warn').hide();
  jQuery('#notice').show();
  jQuery('#notice_span').html('Could not delete <%= @race.name %>. It may have results.');
<% end %>
