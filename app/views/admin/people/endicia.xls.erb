Name	Company	Address1	Address2	City	State	Zip
<%- @people.each do |person| -%>
<%= to_excel person["first_name"] %> <%= to_excel person["last_name"] -%> <%= person["road_number"] -%> <%= !person["card_printed_at"].nil? -%> <%= person["wants_email"] -%>
	<%= person["street"] -%>
	
	<%= to_excel person["city"] -%>
	<%= to_excel person["state"] -%>
  <%# Last column is slightly different to ensure a line break -%>
	<%= person["zip"] %>
<%- end -%>
